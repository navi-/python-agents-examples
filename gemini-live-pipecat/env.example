# Google Gemini API
GEMINI_API_KEY=

# Gemini Model Configuration
# Use Gemini's native audio model for real-time speech capabilities
GEMINI_MODEL=models/gemini-2.5-flash-native-audio-preview-12-2025

# Gemini Voice (Aoede, Charon, Fenrir, Kore, Puck)
GEMINI_VOICE=Puck

# Plivo Credentials
PLIVO_AUTH_ID=
PLIVO_AUTH_TOKEN=

# Public URL for webhooks (ngrok URL or production domain)
# Example: https://abc123.ngrok-free.app
PUBLIC_URL=

# Server port
SERVER_PORT=8080

# System prompt (leave empty to use default helpful assistant)
# SYSTEM_PROMPT=Your custom prompt here
